{
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "no-unused-vars": 1,
        "react/jsx-uses-vars": 2
    }
}
